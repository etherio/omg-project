{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/admin",
      "destination": "https://omg-admin.vercel.app"
    },
    {
      "source": "/admin/:match*",
      "destination": "https://omg-admin.vercel.app/:match*"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/:match*",
      "destination": "/index.html"
    }
  ]
}
